<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net4.0;net4.8;netstandard2.0;netstandard2.1;netcoreapp3.0;netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
		<Authors>Tom</Authors>
		<LangVersion>10.0</LangVersion>
		<ImplicitUsings>disable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<AssemblyVersion>$(Version)</AssemblyVersion>
		<FileVersion>$(Version)</FileVersion>
		<Version>2.0.5</Version>
		<Copyright>Tom 2021</Copyright>
		<Description>便捷的HttpClient库
支持GET/POST/PUT/Delete 上传文件/文件参数
===========================
使用HttpWebRequest实现</Description>
		<PackageId>Tom.HttpLib</PackageId>
		<PackageIcon>Logo.png</PackageIcon>
		<RepositoryUrl>https://github.com/Haku-Men/HttpLib</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>Http;HttpLib;HttpHelper;HttpClient</PackageTags>
		<PackageReleaseNotes>1.修复了PostMan上传文件问题
2.新增文件下载
3.升级框架优化</PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/Haku-Men/HttpLib</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\Logo.png" Pack="true" PackagePath="" />
		<None Include="..\README.md" Pack="true" PackagePath="" />
	</ItemGroup>
</Project>
